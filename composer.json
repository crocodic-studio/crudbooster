{
    "name": "crocodicstudio/crudbooster",
    "description": "CRUDBooster Laravel CRUD Generator, Make a Web Application Just In Minutes, Even With Less Code and fewer Steps !",
    "type": "project",
    "license": "MIT",
    "keywords":[
        "laravel",
        "crudbooster",
        "crud",
        "generator"
    ],
    "scripts": {
        "post-package-install": [
            "php src/installer.php",            
            "php artisan vendor:publish",
            "php artisan vendor:publish --force --provider=\"crocodicstudio\\crudbooster\\CRUDBoosterServiceProvider\"",
            "php artisan migrate --seed"
        ],
        "post-package-update": [
            "php src/installer.php",                        
            "php artisan vendor:publish --force --provider=\"crocodicstudio\\crudbooster\\CRUDBoosterServiceProvider\"",
            "php artisan migrate --seed"
        ]
    },
    "authors": [
        {
            "name": "Ferry",
            "email": "ferry@crocodic.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":">=5.5.9",
        "barryvdh/laravel-dompdf": "0.6.*",
        "doctrine/dbal": "~2.3",
        "laravelcollective/bus": "^5.2",
        "maatwebsite/excel": "~2.1.0",                
        "unisharp/laravel-filemanager": "^1.6",
        "intervention/imagecache":"2.*"
    },
    "autoload": {
        "psr-4": {
          "crocodicstudio\\crudbooster\\": "src/"
        }
      }
}
